{
  "name": "Mournfully's Extension",
  "version": "1.1",
  "manifest_version": 3,
  "description": "An amalgamation of a couple distinct chrome extensions under one unified interface",
  "homepage_url": "https://github.com/mournfully",
  "icons": {
    "16": "static/icon16.png",
    "48": "static/icon48.png",
    "128": "static/icon128.png"
  },
  "action": {
    "default_icon": "static/icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "clipboardWrite"
  ]
}
